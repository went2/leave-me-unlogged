{
  "manifest_version": 3,
  "name": "Leave me Unlogged",
  "version": "0.8.7",
  "description": "关闭自动弹出的登陆弹窗。Do not remind me to log in",
  "content_scripts": [
    {
      "js": ["scripts/index.js"],
      "matches": [
        "https://www.zhihu.com/question/*",
        "https://blog.csdn.net/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}